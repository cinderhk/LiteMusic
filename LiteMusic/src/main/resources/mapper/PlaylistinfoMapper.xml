<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cinderhk.litemusic.mapper.PlaylistinfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cinderhk.litemusic.entity.Playlistinfo">
        <result column="id" property="id" />
        <result column="musicrid" property="musicrid" />
    </resultMap>
    <select id="listById" resultMap="BaseResultMap">
        SELECT *
        FROM playlistinfo
        WHERE id = #{id}
    </select>
</mapper>
